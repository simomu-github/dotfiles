{
  "vim.normalModeKeyBindings": [
      {
          "before": ["<S-h>"],
          "after": ["^"]
      },
      {
          "before": ["<S-l>"],
          "after": ["$", "l"]
      },
      {
          "before": ["j"],
          "after": ["g", "j"]
      },
      {
          "before": ["k"],
          "after": ["g", "k"]
      }
  ]
}
